{
  "name": "api",
  "version": "1.4.8",
  "description": "Api para Gestion de Clases",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon index.js",
    "lint": "eslint .",
    "release": "standard-version && git push -f --follow-tags --no-verify",
    "prepare": "husky",
    "pre-commit": "npm run lint -fix",
    "build": "rimraf dist && mkdir dist && cpy index.js dist && cpy app/**/* dist/app --parents && cpy package.json dist && cpy ca.pem dist"
  },
  "keywords": [
    "Gestion",
    "Clases"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "author": "Ariel Calix",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "he": "^1.2.0",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.21",
    "mysql2": "^3.11.3",
    "nodemailer": "^6.9.16",
    "sequelize": "^6.37.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.14.0",
    "cpy-cli": "^5.0.0",
    "eslint": "^9.14.0",
    "globals": "^15.12.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "rimraf": "^6.0.1",
    "standard-version": "^9.5.0"
  }
}
